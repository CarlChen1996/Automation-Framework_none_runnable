# The folder is used to store those unittest for framework, such as: Test_analyzer, Test_assemble_engine, Test_execution_engine, etc.

# Code outline
    Test_analyzer.py
        class AnalyzerTest(unittest.TestCase)
            setUp(self)
            tearDown(self)
            test_load_excel(self, excel_read)
            test_read_excel(self)

    Test_assemble_engine.py
        class AssembleEngineTest(unittest.TestCase)
            setUp(self)
            scan_folder(self)
            test_1_scan_folder(self)
            test_2_get_task_when_task_exist(self, generate_task)
            test_3_get_task_when_task_not_exist(self, generate_task, sleep_mock)
            test_4_send_task_to_execution(self, send_task)
            test_5_get_ack_right_from_execution_engine(self, remove)
            test_6_get_ack_wrong_from_execution_engine(self, remove)
            test_7_remove_task_from_assemble_queue(self)
            test_8_initial_task(self, insert_task, set_state)
            test_9_add_task_to_assemble_queue(self)

	Test_execution_engine.py
	    class ExecutionEngineTest(unittest.TestCase)
	        setUp(self)
